{
  "private": true,
  "name": "@dremio/dremio-js",
  "version": "0.0.1",
  "description": "JavaScript library for the Dremio API",
  "keywords": [
    "dremio"
  ],
  "author": "Dremio",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "type": "module",
